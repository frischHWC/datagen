{
  "schema_version": 1,
  "name": "DATAGEN",
  "version": "DATAGEN_FULL_VERSION",
  "setActiveSymlink": true,
  "extraVersionInfo": {
    "patchCount": "0",
    "fullVersion": "DATAGEN_FULL_VERSION",
    "baseVersion": "DATAGEN_FULL_NAME"
  },

  "provides": [
    "datagen"
  ],

  "scripts": {
    "defines": "datagen_env.sh"
  },

  "components": [
    {
      "name"       : "datagen",
      "version"    : "DATAGEN_FULL_VERSION",
      "pkg_version": "DATAGEN_FULL_VERSION",
      "pkg_release": "DATAGEN_FULL_NAME"
    }
  ],
  "packages": [
    {
      "name": "datagen",
      "version": "DATAGEN_FULL_VERSION"
    }
  ],

  "users": {
    "datagen": {
      "longname"    : "Data Generator",
      "home"        : "/home/datagen",
      "shell"       : "/bin/bash",
      "extra_groups": [ "hadoop" ]
    }
  },

  "groups": [
    "datagen"
  ]
}