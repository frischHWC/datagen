{
  "Fields": [
    {
      "name": "city",
      "type": "CITY",
      "filters": ["France"]
    },
    {
      "name": "measure_time",
      "type": "TIMESTAMP"
    },
    {
      "name": "lat",
      "type": "LINK",
      "conditionals": {
        "link": "$city.lat"
      }
    },
    {
      "name": "long",
      "type": "LINK",
      "conditionals": {
        "link": "$city.long"
      }
    },
    {
      "name": "wind_provenance",
      "type": "STRING",
      "possible_values": ["NORTH", "EAST", "WEST", "SOUTH"]
    },
    {
      "name": "wind_force",
      "type": "INTEGER",
      "min": 0,
      "max": 120
    },
    {
      "name": "pressure",
      "type": "INTEGER",
      "min": 1000,
      "max": 1020
    },
    {
      "name": "humidity",
      "type": "INTEGER",
      "min": 0,
      "max": 100
    },
    {
      "name": "temperature",
      "type": "INTEGER",
      "min": -10,
      "max": 35
    }
  ],
