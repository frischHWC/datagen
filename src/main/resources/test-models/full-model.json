{
  "Fields": [
    {
      "name": "size",
      "type": "INTEGER"
    },
    {
      "name": "category",
      "type": "INTEGER",
      "possible_values": [0, 1, -1]
    },
    {
      "name": "department",
      "type": "STRING",
      "possible_values": ["hr", "consulting", "marketing", "finance"]
    },
    {
      "name": "country_of_provenance",
      "type": "STRING",
      "possible_values_weighted": {
        "France": 30,
        "Germany": 70
      }
    },
    {
      "name": "percentage",
      "type": "INTEGER",
      "min": 0,
      "max": 100
    },
    {
      "name": "abbreviation",
      "type": "STRINGAZ",
      "length": 3
    },
    {
      "name": "randomName",
      "type": "STRING"
    },
    {
      "name": "userEmail",
      "type": "EMAIL"
    },
    {
      "name": "limitedName",
      "type": "STRING",
      "length": 10
    },
    {
      "name": "bool",
      "type": "BOOLEAN"
    },
    {
      "name": "progression",
      "type": "FLOAT"
    },
    {
      "name": "bigSize",
      "type": "LONG"
    },
    {
      "name": "startDate",
      "type": "TIMESTAMP"
    },
    {
      "name": "bytesLittleArray",
      "type": "BYTES",
      "length" : 10
    },
    {
      "name": "bytesArray",
      "type": "BYTES"
    },
    {
      "name": "hash",
      "type": "HASHMD5"
    },
    {
      "name": "restrictedHash",
      "type": "HASHMD5",
      "length" : 16
    },
    {
      "name": "birthdate",
      "type": "BIRTHDATE"
    },
    {
      "name": "birthdate_old",
      "type": "BIRTHDATE",
      "min": "1/1/1900",
      "max": "1/1/1950"
    },
    {
      "name": "name",
      "type": "NAME",
      "filters": ["France"]
    },
    {
      "name": "country",
      "type": "COUNTRY"
    },
    {
      "name": "longPercent",
      "type": "LONG",
      "min": 0,
      "max": 100
    },
    {
      "name": "onePlusOne",
      "type": "INCREMENT_INTEGER"
    },
    {
      "name": "onePlusTwo",
      "type": "INCREMENT_LONG"
    },
    {
      "name": "formula_1",
      "type": "STRING",
      "conditionals": {
        "formula": "2 * $longPercent + 42 - $onePlusOne"
      }
    },
    {
      "name": "condition_2",
      "type": "STRING",
      "conditionals": {
        "$category=0" : "first_cond",
        "$bool=true" : "second_cond",
        "$category=1 & $longPercent<50" : "third_cond",
        "$country!GERMANY | $longPercent>$onePlusTwo" : "fourth_cond",
        "default" : "default"
      }
    },
    {
      "name": "city",
      "type": "CITY",
      "filters": ["France", "Spain"]
    },
    {
      "name": "city_lat",
      "type": "LINK",
      "conditionals": {
        "link": "$city.lat"
      }
    },
    {
      "name": "city_long",
      "type": "LINK",
      "conditionals": {
        "link": "$city.long"
      }
    },
    {
      "name": "city_country",
      "type": "LINK",
      "conditionals": {
        "link": "$city.country"
      }
    },
    {
      "name": "recording_time",
      "type": "INTEGER",
      "min": 1606229803,
      "max": 1637764585
    },
    {
      "name": "ip",
      "type": "IP"
    },
    {
      "name": "person",
      "type": "CSV",
      "filters": ["country=France"],
      "file": "src/main/resources/dictionaries/person_test.csv",
      "field": "name"
    },
    {
      "name": "person_department",
      "type": "LINK",
      "conditionals": {
        "link": "$person.department"
      }
    },
    {
      "name": "phone_number",
      "type": "PHONE",
      "filters": ["France"]
    },
    {
      "name": "uuid",
      "type": "UUID"
    },
    {
      "name": "email_from_name",
      "type": "STRING",
      "conditionals": {
        "injection": "toto_${name}@our_company_name.${country}"
      }
    }
  ],
  "Table_Names": [
